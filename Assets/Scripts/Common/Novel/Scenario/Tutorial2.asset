  - _speaker: "\u3071\u3063\u3061\u3071\u3063\u3061\u535A\u58EB"
    _text: "\u305D\u3053\u306B\u91CE\u86EE\u306A\u30AA\u30FC\u30AF\u304C\u898B\u3048\u308B\u30D1\u30C1\u306D"
  - _speaker: "\u3042\u306A\u305F"
    _text: "\u30AA\u30FC\u30AF\u3060\u3063\u3066\uFF01\uFF01\uFF01\n\u4ECA\u3059\u3050\u9003\u3052\u306A\u304D\u3083\uFF01\uFF01"
  - _speaker: "\u3071\u3063\u3061\u3071\u3063\u3061\u535A\u58EB"
    _text: "\u843D\u3061\u7740\u304F\u30D1\u30C1\n\u653B\u6483\u306E\u4ED5\u65B9\u3092\u6559\u3048\u308B\u304B\u3089\u5927\u4E08\u592B\u30D1\u30C1"
  - _speaker: "\u3042\u306A\u305F"
    _text: "\u672C\u5F53\u304B\uFF1F\u30AA\u30FC\u30AF\u306B\u901A\u7528\u3059\u308B\u3068\u601D\u3048\u306A\u3044\u306E\u3060\u304C"
  - _speaker: "\u3071\u3063\u3061\u3071\u3063\u3061\u535A\u58EB"
    _text: "\u305D\u3093\u306A\u3053\u3068\u306F\u306A\u3044\u30D1\u30C1\uFF01\n\u541B\u304C\u3053\u306E\u4E16\u754C\u306B\u6765\u305F\u6642\u306B\u6295\u5C04\u546A\u6CD5\u3092\u4F7F\u3048\u308B\u3088\u3046\u306B\u3057\u3066\u304A\u3044\u305F\u30D1\u30C1\n\u6575\u306B\u5411\u304B\u3063\u3066\u300C\u30B9\u30DA\u30FC\u30B9\u300D\u30AD\u30FC\u3092\u62BC\u3057\u3066\u307F\u308B\u30D1\u30C1\uFF01\uFF01"
  - _speaker: "\u3042\u306A\u305F"
    _text: "\u3053\u3046\u306A\u3063\u305F\u3089\u3084\u308B\u3057\u304B\u306A\u3044\u304B"
